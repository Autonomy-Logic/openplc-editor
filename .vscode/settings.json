  "cSpell.words": [
    "Dropup",
    "electronmon",
    "Embla",
    "headlessui",
    "hookform",
    "IFBD",
    "immer",
    "IPLC",
    "ISFC",
    "mimetypes",
    "openplc",
    "oplc",
    "Pous",
    "serviceworker",
    "Shadcn",
    "tanstack",
    "titlebar",
    "toastify"
  ]
}
